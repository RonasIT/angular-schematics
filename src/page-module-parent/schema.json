{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsPageModuleParent",
  "title": "Page Module Parent Schema",
  "type": "object",
  "properties": {
    "parent": {
      "description": "The name of the parent page.",
      "type": "string",
      "minLength": 1,
      "x-prompt": "What is the name of the parent page?"
    },
    "section": {
      "description": "The name of the section.",
      "minLength": 1,
      "type": "string"
    },
    "intoParent": {
      "type": "boolean",
      "description": "When true, generates page into parent page.",
    },
    "name": {
      "description": "The name of the page.",
      "minLength": 1,
      "type": "string"
    },
    "store": {
      "type": "boolean",
      "description": "When true, installs store for the page."
    },
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path to create the page.",
      "visible": false
    }
  },
  "required": [
    "parent",
    "section",
    "name",
    "store"
  ]
}